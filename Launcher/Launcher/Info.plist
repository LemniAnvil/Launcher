<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLName</key>
			<string>LemniAnvilLauncherURL</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>LemniAnvil-launcher</string>
			</array>
		</dict>
	</array>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSExceptionDomains</key>
		<dict>
			<key>textures.minecraft.net</key>
			<dict>
				<key>NSIncludesSubdomains</key>
				<true/>
				<key>NSTemporaryExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
		</dict>
	</dict>
	<key>CurseForgeAPIKey</key>
	<string>$(CURSEFORGE_API_KEY)</string>
	<key>MicrosoftClientID</key>
	<string>$(MICROSOFT_CLIENT_ID)</string>
</dict>
</plist>
